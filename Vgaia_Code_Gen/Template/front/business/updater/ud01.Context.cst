import jp.co.okasan.cmn.base.BaseContext;
import jp.co.okasan.data.<%=this.EntityName%>;
import jp.co.okasan.data.BaseUser;

/**
 * <%=this.Prefix%><%=this.UpdaterCode%>Context Class's Description. <br>
 * 
 * @author: <%=Author%> <%=this.GetCurrentDate()%> Create New
 */
public class <%=this.Prefix%><%=this.UpdaterCode%>Context extends BaseContext<<%=this.EntityName%>> {

	////////// Class Variables //////////
	
	/** data list */
	private List<<%=this.EntityName%>> dataList;
	
	/** Current data */
	private <%=this.EntityName%> currentData;
	
	/** Update counter */
	private int updateResult;
	
	/**
	 * Constructor. <br>
	 * 
	 * @param Nothing
	 * @exception Nothing
	 */
	public <%=this.Prefix%><%=this.UpdaterCode%>Context(BaseUser user, Connection connection) {
		super(user, connection);
	}
	
	/**
	 * @return the dataList
	 */
	public List<<%=this.EntityName%>> getDataList() {
		return dataList;
	}

	/**
	 * @param dataList the dataList to set
	 */
	public void setDataList(List<<%=this.EntityName%>> dataList) {
		this.dataList = dataList;
	}
	
	/**
	 * @return the currentData
	 */
	public <%=this.EntityName%> getCurrentData() {
		return currentData;
	}

	/**
	 * @param currentData the currentData to set
	 */
	public void setCurrentData(<%=this.EntityName%> currentData) {
		this.currentData = currentData;
	}
	
	/**
	 * @return the updateResult
	 */
	public int getUpdateResult() {
		return updateResult;
	}

	/**
	 * @param updateResult the updateResult to set
	 */
	public void setUpdateResult(int updateResult) {
		this.updateResult = updateResult;
	}
}
